{
  "name": "nkowaokwu_twitter",
  "version": "1.1.0",
  "description": "Twitter bot for @nkowaokwu",
  "main": "src/server.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node -- ./src/server.js",
    "build": "./node_modules/.bin/babel -d build/ ./src -s",
    "start": "node ./build/server.js",
    "start:docker": "docker-compose up",
    "mocha": "cross-env NODE_ENV=test mocha --require @babel/register --timeout 10000 --require esm ./tests",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit",
      "commit-msg": "yarn commit-msg"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ijemmao/nkowaokwu_twitter.git"
  },
  "keywords": [
    "twitter",
    "bot"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ijemmao/nkowaokwu_twitter/issues"
  },
  "homepage": "https://github.com/ijemmao/nkowaokwu_twitter#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "esm": "^3.2.25",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7"
  },
  "standard-version": {
    "skip": {
      "changelog": true,
      "tag": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "https": "^1.0.0",
    "lodash": "^4.17.21",
    "mocha": "^8.3.2",
    "node-cron": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "twit": "^2.2.11",
    "urlencode": "^1.1.0"
  }
}
